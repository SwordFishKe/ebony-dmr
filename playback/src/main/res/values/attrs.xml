 <!-- Nav-PageControl -->
    <declare-styleable name="navPageControl">
        <!-- The padding for top -->
        <attr name="navPageControlTopPadding" format="dimension"/>
        <!-- The padding for bottom -->
        <attr name="navPageControlBottomPadding" format="dimension"/>
        <!-- The padding for Right -->
        <attr name="navPageControlRightPadding" format="dimension"/>
        <!-- The padding for left -->
        <attr name="navPageControlLeftPadding" format="dimension"/>
        <!-- The width of the navPageontrol -->
        <attr name="navPageControlWidth" format="dimension"/>
        <!-- The height of the navPageontrol -->
        <attr name="navPageControlHeight" format="dimension"/>
        <!-- The radius of the normal dot -->
        <attr name="navPageControlcircleRadNormal" format="dimension"/>
        <!-- The radius of the selected dot -->
        <attr name="navPageControlcircleRadLarge" format="dimension"/>
        <!-- The size of the small   dot -->
        <attr name="navPageControlcircleRadSmall" format="dimension"/>
