        <attr name="navPageControlcirclepadding" format="dimension"/>
        <!-- The total number of pages in Nav-Page control -->
        <attr name="totalNumPages" format="integer" />
        <!-- The current page in Page control -->
        <attr name="currentPage" format="integer"></attr>
        <!-- navPageControl_navPageControlColor -->
        <attr name="navPageControlDefaultColor" format="color"></attr>
        
        <attr name="navPageControlSelectedColor" format="color"></attr>
        
        <attr name="navPageControlFishEyeColor" format="color"></attr>
    
    </declare-styleable>
    
